<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	
	<bean id="AbstractPet" class="fox.pets.AbstractPet">
    	<constructor-arg index="0" value=""/>
    	<constructor-arg index="1" value=0f/>
    	<constructor-arg index="2" value=Pet.SEX.MALE/>
    	<constructor-arg index="3" value=""/>
	</bean>
	
	
	<bean id="beanOwner" class="fox.clients.Owner">
    	<constructor-arg name="name" value="" />
    	<constructor-arg index="1" ref="beanPet[]"/>
	</bean>
	
	<bean id="beanDoctor" class="fox.doctors.Doctor">
    	<constructor-arg name="name" value="" />
    	
	</bean>	
	
	<bean id="beanClinic" class="fox.clinics.PetClinic">
    	<constructor-arg name="name" value=""/>
    	
	</bean>
	
</beans>